<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
	   <setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	<typeAliases>
		<typeAlias type="kr.co.acctmgmt.domain.User" alias="User" />
		<typeAlias type="kr.co.acctmgmt.domain.Employee" alias="Employee"/>
		<typeAlias type="kr.co.acctmgmt.domain.Config" alias="Config"/>
		<typeAlias type="kr.co.acctmgmt.domain.SBGTCDDomain" alias="SBGTCD"/>
		<typeAlias type="kr.co.acctmgmt.domain.BgtICF" alias="BgtICF"/>
	</typeAliases>
	<mappers>
		<mapper resource="mapper/UserMapper.xml" />
		<mapper resource="mapper/SBGTCDMapper.xml"/>
		<mapper resource="mapper/EmployeeMapper.xml"/>
		<mapper resource="mapper/ConfigMapper.xml"/>
		<mapper resource="mapper/BgtICFMapper.xml"/>
	</mappers>
</configuration>